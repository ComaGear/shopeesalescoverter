<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" name="shopeesalescoverter" packages="">

    <level value="info"/>.

    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>

    <Appenders>
        <File name="fout" fileName="shopeeSalesCoverter.log" append="true">
            <PatternLayout>
                <Pattern>%d{yyyy-MM-dd HH:mm:ss} %-5p %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>


    <Loggers>
        <Root level="error">
            <AppenderRef ref="stdout"/>
        </Root>
        <Root level="INFO">
            <AppenderRef ref="stdout"/>
            <AppenderRef ref="fout"/>
        </Root>
        <!-- <Logger name="com.colbertlum.Imputer.Utils" level="trace">
            <AppenderRef ref="stdout"/>
        </Logger> -->
    </Loggers>
</Configuration>